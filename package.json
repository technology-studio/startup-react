{
  "name": "@txo/startup-react",
  "version": "3.0.16",
  "description": "Technology Studio - Startup react",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/startup-react.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx .",
    "fixcode": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky install && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint && yarn build && tsc --noEmit && yarn test && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@txo/redux": "^4.0.27",
    "redux-saga": "^1.2.3"
  },
  "peerDependencies": {
    "@txo-peer-dep/redux": "^1.0.1",
    "@txo-peer-dep/startup-react": "^1.0.2",
    "immutability-helper": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@txo-peer-dep/log": "^4.0.2",
    "@txo-peer-dep/redux": "^1.0.1",
    "@txo-peer-dep/startup-react": "^1.0.2",
    "@txo/log-console": "^3.0.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "eslint-config-txo-typescript-react": "^1.2.54",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "husky": "^8.0.3",
    "immutability-helper": "^3.1.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.1",
    "npm-check-updates": "^16.14.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux": "^4.2.1",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.5",
    "semantic-release-slack-bot": "^4.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.28"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
